  "description": "A CMS for Ali",
  "scripts": {
    "sass": "sass --style=compressed scss/styles.scss static/css/styles.css",
    "watch:eleventy": "npx eleventy --serve",
    "watch:sass": "npm run sass -- --watch",
    "build": "npm run sass && npx eleventy",
