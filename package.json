    "dev": "npm-run-all sass --parallel watch:*",
    "scale": "node scale.js",
    "fixpaths": "node fixFilePaths.js",
    "oldbuild": "npm run scale && npm run sass && npx eleventy",
    "debug": "DEBUG=* npx eleventy"
  },
